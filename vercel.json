{
  "buildCommand": "pnpm build:client",
  "outputDirectory": "dist/spa",
  "installCommand": "corepack enable && pnpm install --frozen-lockfile",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20",
      "COREPACK_ENABLE_STRICT": "0"
    }
  }
}